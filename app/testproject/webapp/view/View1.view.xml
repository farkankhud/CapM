<mvc:View controllerName="com.Employee.testproject.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:plugins="sap.ui.table.plugins"
	xmlns:table="sap.ui.table"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}" class="pClass sapUiSizeCompact">
        <content>
        <!-- <Panel expandable="true" headerText="Sales Order Display " width="auto" class="sapUiResponsiveMargin">
        <content> -->
        <table:Table id="firstTable" class="plant1WTable" rows="{path: '/Interactions_Header'}">
			<table:plugins>
				<plugins:MultiSelectionPlugin limit="{path: 'config>/limit', type: 'sap.ui.model.type.Integer'}" enableNotification="true"
				  showHeaderSelector="{config>/showHeaderSelector}" selectionChange="onRowSelect"/>
				 </table:plugins>
  <table:columns>
     <table:Column hAlign="Center">
			<table:label>
					<Label text="Employee Id" design="Bold"/>
			</table:label>
				<table:template>
							<Text text="{ID}"></Text>
				</table:template>
	</table:Column>
	<table:Column hAlign="Center">
			<table:label>
					<Label text="Employee Name" design="Bold"/>
			</table:label>
			    <table:template>
						<Text text="{EmplName}"></Text>
				</table:template>
	</table:Column>

    <table:Column hAlign="Center">
			<table:label>
					<Label text="Department" design="Bold"/>
			</table:label>
			    <table:template>
						<Text text="{Dept}"></Text>
				</table:template>
	</table:Column>

    <table:Column hAlign="Center">
			<table:label>
					<Label text="Salary" design="Bold"/>
			</table:label>
			    <table:template>
						<Text text="{Salary}"></Text>
				</table:template>
	</table:Column>

   </table:columns>

        </table:Table>
        
        </content>
        <!-- </Panel>
        </content> -->
    </Page>
</mvc:View>
